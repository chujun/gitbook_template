{
  "title": "gitbook",
  "author": "jun.chu",
  "description": "gitbook book",
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "styles": {
    "website": "./styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {
    "sidebar": {
      "github": "https://github.com/chujun"
    }
  },
  "plugins": [
    "-sharing",
    "sharing-plus",

    "splitter",

    "chapter-fold",

    "anchors",
    "anchor-navigation-ex",

    "github",
    "github-buttons",

    "donate",

    "favicon",

    "code",
    "advanced-emoji",

    "accordion",

    "-lunr",
    "-search",
    "search-pro",
    "uml"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/chujun"
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "chujun",
          "repo": "gitbook",
          "type": "star",
          "size": "small",
          "count": true
        }
      ]
    },
    "donate": {
      "wechat": "/img/alipay.jpg",
      "alipay": "/img/wxpay.jpg",
      "title": "",
      "button": "赞赏",
      "alipayText": " ",
      "wechatText": " "
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": false,
      "whatsapp": false,
      "all": [
        "google",
        "facebook",
        "weibo",
        "twitter",
        "qq",
        "qzone",
        "linkedin",
        "pocket"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "favicon": {
      "shortcut": "./source/images/favicon.jpg",
      "bookmark": "./source/images/favicon.jpg",
      "appleTouch": "./source/images/apple-touch-icon.jpg",
      "appleTouchMore": {
        "120x120": "./source/images/apple-touch-icon.jpg",
        "180x180": "./source/images/apple-touch-icon.jpg"
      }
    },
    "uml": {
        "format": "png",
        "nailgun": false
    }
  }
}